asic\r\n          onClose={() => setHostModalOpenStatus(false)}\r\n          onOpen={() => setHostModalOpenStatus(true)}\r\n          open={hostModalOpenStatus}\r\n          size='small'\r\n          trigger={<button className=\"ui button massive joinScreenPlayButton\">Host Game</button>}>\r\n          <Header icon><Icon className='large users' />\r\n              Host Game\r\n              <br></br>\r\n              <h1>Room Code: 012345</h1>\r\n          </Header>\r\n          <Modal.Actions className=\"joinScreenModalButtonContainer\">\r\n            <Link to={{ pathname: \"/lobby/012345\" }}>\r\n              <Button color=\"teal\" onClick={hostGame}><Icon name='users'/>Host!</Button>\r\n            </Link>\r\n            <Button color=\"secondary\"><Icon name='setting'/>Options</Button>\r\n            <Button inverted color='red' onClick={() => setHostModalOpenStatus(false)}><Icon name='remove' />Close</Button>\r\n          </Modal.Actions>\r\n        </Modal>\r\n\r\n      </div>\r\n\r\n      {/* Bottom Third */}\r\n      <div className=\"joinScreenBottomThird\">\r\n        <p>by Chris Hopkins, Jacob Gruza &amp; Tian Brown</p>\r\n        <p>&#169; 2021 Game Foot Enterprises</p>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default JoinScreen;\r\n","F:\\Personal Projects\\GameFoot\\game-foot-app\\src\\screens\\LobbyScreen.js",[],"F:\\Personal Projects\\GameFoot\\game-foot-app\\src\\screens\\VoteScreen.js",[],"F:\\Personal Projects\\GameFoot\\game-foot-app\\src\\screens\\LoadScreen.js",[],"F:\\Personal Projects\\GameFoot\\game-foot-app\\src\\screens\\ResultsScreen.js",[],["42","43"],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":7,"column":40,"nodeType":"50","messageId":"51","endLine":7,"endColumn":48},{"ruleId":"48","severity":1,"message":"52","line":14,"column":10,"nodeType":"50","messageId":"51","endLine":14,"endColumn":19},{"ruleId":"44","replacedBy":"53"},{"ruleId":"46","replac